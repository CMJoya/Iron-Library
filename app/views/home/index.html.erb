<div class="page-header">
  <h1>The Library</h1>
</div>

<% if @current_user.present? %>
  <%= @current_user.username %>
  <%= link_to "Log Out", log_out_path, method: :delete %>
<% else %>
  <%= link_to "Log In", log_in_path, class: "btn btn-success btn-lg btn-block" %>
<% end %>


